# To set in CONVEX DASHBOARD:
# Civitai API Key
CIVITAI_API_KEY=

# Shared secret for authenticating Convex -> Worker and Worker -> Convex calls
ASSETS_SECRET=

# Full URL of the Cloudflare Worker endpoint that Convex calls to enqueue tasks
ASSETS_WORKER_URL=

# Optional: R2 Credentials if using checkAsset directly from Convex
# R2_ENDPOINT=
# R2_ACCESS_KEY_ID=
# R2_SECRET_ACCESS_KEY=
# R2_BUCKET_NAME=

# To set in DEVELOPMENT OR PRODUCTION ENVIRONMENT:
# Convex Deployment Info (usually managed by `npx convex deploy`)
CONVEX_DEPLOYMENT=
CONVEX_URL=

